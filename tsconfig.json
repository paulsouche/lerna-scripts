{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ES2015"],
    "declaration": false,
    "sourceMap": false,
    "outDir": "./dist",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"],
    "types": ["node"]
  }
}